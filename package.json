{
  "name": "react-delay-render",
  "version": "0.0.2",
  "description": "a component that delays the rendering of child components",
  "main": "./lib/DelayRender.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require ./test/setup.js --recursive",
    "watch:test": "npm t -- -w",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm t",
    "start": "webpack-dev-server --inline --hot",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "compile": "babel --presets es2015,react -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "arnthor3/react-delay-render"
  },
  "keywords": [
    "react"
  ],
  "author": "arnthor agustsson",
  "license": "ISC",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "nyc": "^7.1.0-candidate3",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
